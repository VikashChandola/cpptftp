#ifndef __PROJECT_CONFIG_HPP__
#define __PROJECT_CONFIG_HPP__
// Project configuration file

// Enables extensive logging. This prints out execution flow in console. This is different from syslog.
// These logs are not sent to syslog
#mesondefine CONF_EXTENSIVE_DEBUG_LOG

// Amount of duration application will wait before sending next packet. This parameter is relevant only if
// CONF_DELAY_SEND is enabled
#mesondefine CONF_CONSTANT_DELAY_DURATION

// Default network timeout(in milliseconds)
#mesondefine CONF_NETWORK_TIMEOUT

// Max retry before bailing on a given peer
#mesondefine CONF_MAX_RETRY_COUNT

#endif
